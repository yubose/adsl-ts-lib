{
  "author": "Christopher Tran <pfftdammitchris@gmail.com>",
  "dependencies": {
    "noodl-core": "^0.1.22",
    "noodl-types": "^1.0.228",
    "noodl-utils": "^0.0.101"
  },
  "description": "YAML bindings for noodl-core",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "homepage": "",
  "keywords": [
    "noodl",
    "ecos",
    "aitmed",
    "telemedicine",
    "yaml"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "name": "noodl-yaml",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.aitmed.com/pfftdammitchris/aitmed-noodl-lib.git"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "concurrently \"npm run types:watch\" \"rollup -c -w\"",
    "postbuild": "npm run types",
    "start": "npx tsc -w",
    "test": "cross-env NODE_ENV=test ts-mocha -p tsconfig.test.json -r source-map-support/register --extensions ts --watch --watch-files \"src/**/*.ts\" --watch-files \"../noodl-core/dist\" \"src/**/*.test.ts\" --file src/__tests__/setup.ts",
    "test:file": "cross-env NODE_ENV=test ts-mocha -p tsconfig.test.json -r source-map-support/register --extensions ts --watch --watch-files \"src/**/*.ts\" --file src/__tests__/setup.ts",
    "types": "npx tsc --emitDeclarationOnly --esModuleInterop --declaration --declarationDir dist --project tsconfig.json",
    "types:watch": "npm run types -- -w"
  },
  "types": "dist/index.d.ts",
  "version": "0.0.18",
  "gitHead": "0be60bf26d3b63452ca98167c6e95448db3ed79e"
}
